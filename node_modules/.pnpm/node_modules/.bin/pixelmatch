#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/andersson/Projects/game/node_modules/.pnpm/pixelmatch@7.1.0/node_modules/pixelmatch/bin/node_modules:/home/andersson/Projects/game/node_modules/.pnpm/pixelmatch@7.1.0/node_modules/pixelmatch/node_modules:/home/andersson/Projects/game/node_modules/.pnpm/pixelmatch@7.1.0/node_modules:/home/andersson/Projects/game/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/andersson/Projects/game/node_modules/.pnpm/pixelmatch@7.1.0/node_modules/pixelmatch/bin/node_modules:/home/andersson/Projects/game/node_modules/.pnpm/pixelmatch@7.1.0/node_modules/pixelmatch/node_modules:/home/andersson/Projects/game/node_modules/.pnpm/pixelmatch@7.1.0/node_modules:/home/andersson/Projects/game/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pixelmatch/bin/pixelmatch" "$@"
else
  exec node  "$basedir/../pixelmatch/bin/pixelmatch" "$@"
fi
